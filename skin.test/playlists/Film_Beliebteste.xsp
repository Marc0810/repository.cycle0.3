<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<smartplaylist type="movies">
    <name>Filme - Sortierung NutzerRating</name>
    <match>all</match>
    <rule field="userrating" operator="greaterthan">
        <value>5</value>
    </rule>
	<limit>18</limit>
    <order direction="ascending">lastplayed</order>
</smartplaylist>