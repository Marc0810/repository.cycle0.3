<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<smartplaylist type="movies">
    <name>Filme Genre Komödie</name>
    <match>one</match>
    <rule field="genre" operator="contains">
        <value>Comedy</value>
    </rule>
    <rule field="genre" operator="contains">
        <value>Komödie</value>
    </rule>
    <rule field="genre" operator="contains">
        <value>Sitcom</value>
    </rule>
    <order direction="ascending">random</order>
</smartplaylist>
