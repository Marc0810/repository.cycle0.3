<?xml version="1.0" encoding="UTF-8"?>
<window><!-- Cycle 0.4 -->
<backgroundcolor>BackgroundColor</backgroundcolor>
<defaultcontrol always="true">54</defaultcontrol>
<menucontrol>9700</menucontrol>
<controls>
	<control type="button" id="9700">
		<description>Hidden - quicknav menue oeffnen</description>
		<include>HiddenControl</include>
		<onleft>ActivateWindow(1120)</onleft>
		<onright>ActivateWindow(1120)</onright>
		<onup>ActivateWindow(1120)</onup>
		<ondown>ActivateWindow(1120)</ondown>
		<onfocus>ActivateWindow(1120)</onfocus>
	</control>
	<control type="button" id="54">
		<include>HiddenControl</include>
		<onleft>SetFocus(9900)</onleft>
		<onright>SetFocus(9900)</onright>
		<onup condition="!String.IsEmpty(Window(weather).Property(Daily.IsFetched))">51</onup>
		<ondown condition="!String.IsEmpty(Window(weather).Property(Daily.IsFetched))">51</ondown>
	</control>
	<control type="spincontrolex" id="3">
		<description>Change location</description>
		<include>HiddenControl</include>
		<label></label>
	</control>
	<include condition="Skin.HasSetting(EnableVintageOverlay)">Vintagebackground</include>
	<include condition="!Skin.HasSetting(EnableVintageOverlay)">Cyclebackground</include>
	<control type="group"><!-- breadcumb -->
		<include>Topbar</include>
		<animation type="Conditional" condition="Window.IsVisible(Custom_1120_Options.xml)">
			<effect type="slide" end="395,0" delay="180" time="180" tween="quadratic" easing="out" reversible="true" />
		</animation>
	</control>
	<control type="group"><!-- views -->
		<description>Views</description>
		<control type="group" id="50">
			<include>Animation-Conditional-InaktivZoom</include>
			<animation type="Conditional" condition="Window.IsVisible(Custom_1120_Options.xml) | Window.IsVisible(shutdownmenu)">
				<effect type="slide" end="395,0" delay="180" time="180" tween="quadratic" easing="out" reversible="true" />
			</animation>
			<include>WeatherDaily</include>
			<include>WeatherCurrent</include>
			<control type="group"><!-- info kein wetter -->
				<left>760</left>
				<top>520</top>
				<visible>!Weather.IsFetched | IsEmpty(Window(Weather).Property(WeatherProvider))</visible>
				<control type="label">
					<width>400</width>
					<height>200</height>
					<align>center</align>
					<font>weatherloading</font>
					<textcolor>red</textcolor>
					<label>Ë</label>
				</control>
				<control type="label">
					<top>380</top>
					<width>400</width>
					<height>80</height>
					<align>center</align>
					<label>suche Wetterbericht</label>
					<wrapmultiline>true</wrapmultiline>
					<visible>!Weather.IsFetched + !IsEmpty(Window(Weather).Property(WeatherProvider))</visible>
				</control>
				<control type="label">
					<top>380</top>
					<width>400</width>
					<height>80</height>
					<align>center</align>
					<wrapmultiline>true</wrapmultiline>
					<label>kein Addon ist ausgewählt</label>
					<visible>!Weather.IsFetched + IsEmpty(Window(Weather).Property(WeatherProvider))</visible>
				</control>
			</control>
		</control>
	</control>
	<control type="group"><!-- Logo Quicknav 9900 -->
		<description>Logo Quicknav 9900</description>
		<left>760</left>
		<top>340</top>
		<width>400</width>
		<height>400</height>
		<visible allowhiddenfocus="true">Control.HasFocus(9900)</visible>
		<include>Animation-OpenCloseVisibleHidden-Zoom</include>
		<include>Cycle-LogoMenue</include>
		<control type="label">
			<description>Linker Button indikator</description>
			<left>-150</left>
			<top>170</top>
			<width>75</width>
			<height>75</height>
			<align>center</align>
			<font>BoldBig</font>
			<textcolor>$VAR[SecondaryCol]</textcolor>
			<label> ‹‹</label>
			<visible>Control.HasFocus(9900) + Control.IsVisible(9902)</visible>
		</control>
		<control type="label">
			<description>Rechter Button indikator</description>
			<left>450</left>
			<top>170</top>
			<width>75</width>
			<height>75</height>
			<align>center</align>
			<font>BoldBig</font>
			<textcolor>$VAR[SecondaryCol]</textcolor>
			<label> ››</label>
			<visible>Control.HasFocus(9900) + Control.IsVisible(9901)</visible>
		</control>
		<control type="button" id="9900">
			<description>Startbutton ansicht refresh</description>
			<width>400</width>
			<height>400</height>
			<align>center</align>
			<font>IconHome</font>
			<textcolor>$VAR[CirclesCol]</textcolor>
			<focusedcolor>$VAR[CirclesCol]</focusedcolor>
			<onleft>SetFocus(9902)</onleft>
			<onright>SetFocus(9901)</onright>
			<onup>54</onup>
			<ondown>54</ondown>
			<include>Cycle-WeatherMenue-Action</include>
			<label>$VAR[Cycle-WeatherMenue-Icon]</label>
		</control>
		<control type="button" id="9902">
			<description>Linker Button menue</description>
			<left>-600</left>
			<width>400</width>
			<height>400</height>
			<align>center</align>
			<font>IconHomeNF</font>
			<textcolor>$VAR[PrimaryCol]</textcolor>
			<focusedcolor>$VAR[PrimaryCol]</focusedcolor>
			<label>$VAR[Cycle-WeatherMenue-IconL]</label>
			<include>Cycle-WeatherMenue-ActionL</include>
			<include>Animation-Opacity10</include>
		</control>
		<control type="button" id="9901">
			<description>Rechter Button Setting</description>
			<left>600</left>
			<width>400</width>
			<height>400</height>
			<align>center</align>
			<font>IconHome</font>
			<textcolor>$VAR[PrimaryCol]</textcolor>
			<focusedcolor>$VAR[PrimaryCol]</focusedcolor>
			<label>$VAR[Cycle-WeatherMenue-IconR]</label>
			<include>Cycle-WeatherMenue-ActionR</include>
			<include>Animation-Opacity10</include>
		</control>
	</control>
	<control type="group"><!-- logo text -->
		<include>Animation-OpenCloseVisibleHidden-Fade</include>
		<visible>Control.HasFocus(9900)</visible>
		<control type="label">
			<description>Titel skin label</description>
			<left>460</left>
			<top>720</top>
			<width>1000</width>
			<height>60</height>
			<font>CycleLogoBranding</font>
			<textcolor>TextColorMedium</textcolor>
			<align>center</align>
			<label>$VAR[GlobalSkinversion]</label>
		</control>
		<control type="label">
			<description>Subtitel provider</description>
			<left>460</left>
			<top>870</top>
			<width>1000</width>
			<height>60</height>
			<align>center</align>
			<font>BreadcumbNormal</font>
			<textcolor>TextColorMedium</textcolor>
			<label>$INFO[Window(Weather).Property(WeatherProvider)]</label>
		</control>
		<control type="label">
			<description>Subinfo aktualisierungszeit</description>
			<left>460</left>
			<top>930</top>
			<width>1000</width>
			<height>50</height>
			<align>center</align>
			<font>RegularSmall</font>
			<textcolor>TextColorMedium</textcolor>
			<label>$INFO[Window(Weather).Property(Updated),Update: ,]</label>
		</control>
	</control>
</controls>
</window>
