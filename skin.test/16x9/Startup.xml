<control type="group"><!-- Status Fade Text -->
	<description>Status Fade Text</description>
	<visible>!Control.HasFocus(9)</visible>
	<include>Animation-OpenCloseVisibleHidden-Fade</include>
	<control type="label">
		<description>Fadetext1</description>
		<left>460</left>
		<top>880</top>
		<width>1000</width>
		<height>60</height>
		<align>center</align>
		<font>RegularSmall</font>
		<textcolor>TextColorMedium</textcolor>
		<label>bitte warten...</label>
		<visible>!Control.HasFocus(14) + !Control.HasFocus(15)</visible>
		<animation type="Hidden">
			<effect type="slide" start="0,0" end="0,40" time="280" tween="back" easing="out" />
			<effect type="fade" start="100" end="0" time="280" tween="cubic" easing="in" />
		</animation>
	</control>
	<control type="label">
		<description>Fadetext2 - Einblenden wenn Fadetext1 verschwindet</description>
		<left>460</left>
		<top>880</top>
		<width>1000</width>
		<height>60</height>
		<align>center</align>
		<font>RegularSmall</font>
		<textcolor>TextColorMedium</textcolor>
		<label>laden abgeschlossen</label>
		<visible>Control.HasFocus(14) | Control.HasFocus(15)</visible>
		<animation type="Visible" reversible="false">
			<effect type="slide" start="0,40" end="0,0" time="280" tween="back" easing="in" />
			<effect type="fade" start="0" end="100" time="280" tween="cubic" easing="in" />
		</animation>
		<animation type="Hidden">
			<effect type="slide" start="0,0" end="0,40" time="280" tween="back" easing="out" />
			<effect type="fade" start="100" end="0" time="280" tween="cubic" easing="in" />
		</animation>
	</control>
</control>
