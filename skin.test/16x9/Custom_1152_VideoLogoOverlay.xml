			<control type="label">
				<top>60</top>
				<width>1455</width>
				<height>60</height>
				<textcolor>$VAR[OSDDialogCol]</textcolor>
				<label>[B]$INFO[VideoPlayer.Season,Staffel ,: ]$INFO[VideoPlayer.Episode,Episode ,][/B]</label>
				<visible>VideoPlayer.Content(episodes)</visible>
			</control>
			<control type="label">
				<width>1455</width>
				<height>60</height>
