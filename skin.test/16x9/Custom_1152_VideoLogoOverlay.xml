<?xml version="1.0" encoding="UTF-8"?>
<window type="dialog" id="1152"><!-- Cycle 0.4 -->
<visible>Window.IsActive(fullscreenvideo)</visible>
<controls>
<control type="group"><!-- pause -->
	<!-- -->
	<visible>VideoPlayer.IsFullscreen + Player.Paused + System.IdleTime(10)</visible>
	<visible>VideoPlayer.Content(movies) | VideoPlayer.Content(episodes)</visible>
	<visible>!System.HasModalDialog</visible>
	<control type="image">
		<width>screen.width</width>
		<height>screen.height</height>
		<texture colordiffuse="black">white.png</texture>
		<include>Animation-OpenCloseVisibleHidden-Fade</include>
		<include>Animation-Opacity60</include>
	</control>
	<control type="grouplist">
		<left>100</left>
		<top>400</top>
		<width>1720</width>
		<height>680</height>
		<usecontrolcoords>true</usecontrolcoords>
		<animation type="WindowOpen" reversible="false">
			<effect type="zoom" start="150" end="100" center="350,540" time="300" tween="cubic" easing="out" />
			<effect type="fade" start="0" end="100" time="150" />
		</animation>
		<animation type="WindowClose" reversible="false">
			<effect type="fade" start="100" end="0" delay="100" time="100" />
			<effect type="zoom" start="100" end="130" center="350,540" time="200" tween="cubic" easing="in" />
		</animation>
		<animation type="Visible" reversible="false">
			<effect type="zoom" start="150" end="100" center="350,540" time="300" tween="cubic" easing="out" />
			<effect type="fade" start="0" end="100" time="150" />
		</animation>
		<animation type="Hidden" reversible="false">
			<effect type="fade" start="100" end="0" delay="100" time="100" />
			<effect type="zoom" start="100" end="130" center="350,540" time="200" tween="cubic" easing="in" />
		</animation>
		<control type="label">
			<width>1690</width>
			<height>60</height>
			<font>BoldCDNormal</font>
			<textoffsetx>15</textoffsetx>
			<textcolor>$VAR[OSDDialogCol]</textcolor>
			<label>Es läuft</label>
		</control>
		<control type="textbox">
			<left>15</left>
			<width>1690</width>
			<height>auto</height>
			<font>TitleCapsBig</font>
			<textcolor>$VAR[OSDDialogCol]</textcolor>
			<label>$INFO[Player.Title]</label>
			<visible>VideoPlayer.Content(movies)</visible>
		</control>
		<control type="textbox">
			<left>15</left>
			<width>1690</width>
			<height>auto</height>
