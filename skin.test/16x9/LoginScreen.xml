<?xml version="1.0" encoding="UTF-8"?>
<window><!-- Cycle 0.4 -->
<backgroundcolor>BackgroundColor</backgroundcolor>
<defaultcontrol always="true">52</defaultcontrol>
<controls>
<include condition="Skin.HasSetting(EnableVintageOverlay)">Vintagebackground</include>
<include condition="!Skin.HasSetting(EnableVintageOverlay)">Cyclebackground</include>
<control type="wraplist" id="52">
	<description>Benutzerliste</description>
	<left>690</left>
	<top>473</top>
	<width>820</width>
	<height>174</height>
	<onleft>510</onleft>
	<onright>510</onright>
	<onup>510</onup>
	<ondown>510</ondown>
	<focusposition>0</focusposition>
	<scrolltime tween="cubic" easing="inout">450</scrolltime>
	<itemlayout width="874" height="174">
		<control type="group">
			<left>20</left>
			<top>20</top>
			<control type="image">
				<description>Schatten</description>
				<left>-12</left>
				<top>-12</top>
				<width>124</width>
				<height>124</height>
				<texture border="8" colordiffuse="ShadowColor">views/shadow8.png</texture>
			</control>
			<control type="image">
				<description>Border</description>
				<left>-4</left>
				<top>-4</top>
				<width>108</width>
				<height>108</height>
				<texture border="8" colordiffuse="$VAR[CirclesCol]">views/white8.png</texture>
			</control>
			<control type="image">
				<description>Hintergrund</description>
				<width>100</width>
				<height>100</height>
				<texture background="true" colordiffuse="$VAR[PrimaryCol]">white.png</texture>
			</control>
			<control type="label">
				<description>Fallback</description>
				<width>100</width>
				<height>100</height>
				<align>center</align>
				<font>Iconbannerdefault</font>
				<textcolor>$VAR[CirclesCol]</textcolor>
				<label>Q</label>
			</control>
			<control type="image">
				<width>100</width>
				<height>100</height>
				<texture background="true">$INFO[ListItem.Icon]</texture>
				<aspectratio>scale</aspectratio>
			</control>
		</control>
	</itemlayout>
	<focusedlayout width="874" height="174">
		<control type="group">
			<left>20</left>
			<top>20</top>
			<animation type="Focus">
				<effect type="fade" start="70" end="100" time="250" />
				<effect type="zoom" start="80" end="100" center="112,112" time="250" />
			</animation>
			<animation type="UnFocus">
				<effect type="fade" start="100" end="70" time="120" />
				<effect type="zoom" start="100" end="80" center="112,112" time="120" />
			</animation>
			<control type="image">
				<description>Schatten</description>
				<left>-12</left>
				<top>-12</top>
				<width>124</width>
				<height>124</height>
				<texture border="8" colordiffuse="ShadowColor">views/shadow8.png</texture>
			</control>
			<control type="image">
				<description>Border</description>
				<left>-4</left>
				<top>-4</top>
				<width>108</width>
				<height>108</height>
				<texture border="8" colordiffuse="$VAR[CirclesCol]">views/white8.png</texture>
			</control>
			<control type="image">
				<description>Hintergrund</description>
				<width>100</width>
				<height>100</height>
				<texture background="true" colordiffuse="$VAR[PrimaryCol]">white.png</texture>
			</control>
			<control type="label">
				<description>Fallback</description>
				<width>100</width>
				<height>100</height>
				<align>center</align>
				<font>Iconbannerdefault</font>
				<textcolor>$VAR[CirclesCol]</textcolor>
				<label>Q</label>
			</control>
			<control type="image">
				<width>100</width>
				<height>100</height>
				<texture background="true">$INFO[ListItem.Icon]</texture>
				<aspectratio>scale</aspectratio>
			</control>
			<control type="label">
				<description>Benutzername</description>
				<left>130</left>
				<top>10</top>
				<width>770</width>
				<height>30</height>
				<label>$INFO[ListItem.Label]</label>
			</control>
			<control type="label">
				<description>Zuletzt angemeldet</description>
				<left>130</left>
				<top>50</top>
				<width>770</width>
				<height>30</height>
				<font>RegularSmall</font>
				<textcolor>TextColorMedium</textcolor>
				<label>$INFO[ListItem.Label2]</label>
			</control>
		</control>
	</focusedlayout>
</control>
</controls>
</window>