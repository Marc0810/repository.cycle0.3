	<include name="CircleMenue-Layout">
		<param name="CircleMenue-ID"/>
		<definition>
			<itemlayout height="152" width="222">
				<control type="image">
					<include>CircleMenue-Dimension</include>
					<texture colordiffuse="$VAR[CirclesInnerCol]">rings/inner-ring.png</texture>
				</control>
				<control type="image">
					<include>CircleMenue-Dimension</include>
					<texture colordiffuse="$VAR[CirclesCol]">rings/outer-ring.png</texture>
				</control>
				<include>CircleMenue-InfoIcon</include>
			</itemlayout>
			<focusedlayout height="152" width="222">
				<control type="image">
					<include>CircleMenue-Dimension</include>
					<texture colordiffuse="$VAR[SecondaryCol]">rings/inner-ring.png</texture>
					<visible>Control.HasFocus($PARAM[CircleMenue-ID])</visible>
				</control>
				<control type="image">
					<include>CircleMenue-Dimension</include>
					<texture colordiffuse="$VAR[CirclesInnerCol]">rings/inner-ring.png</texture>
					<visible>!Control.HasFocus($PARAM[CircleMenue-ID])</visible>
				</control>
				<control type="image">
					<include>CircleMenue-Dimension</include>
					<texture colordiffuse="$VAR[CirclesCol]">rings/outer-ring.png</texture>
				</control>
				<include>CircleMenue-InfoIcon</include>
			</focusedlayout>
		</definition>
	</include>
	<include name="CircleMenue-AddoninfoLayout">
		<itemlayout width="202">
			<control type="group">
				<top>55</top>
				<control type="image">
					<include>CircleMenue-Dimension</include>
					<texture colordiffuse="$VAR[CirclesInnerCol]">rings/inner-ring.png</texture>
				</control>
				<include>CircleMenue-InfoIcon</include>
				<control type="image">
					<include>CircleMenue-Dimension</include>
					<texture colordiffuse="$VAR[CirclesCol]">rings/outer-ring.png</texture>
				</control>
			</control>
		</itemlayout>
		<focusedlayout width="202">
			<control type="label">
				<width>200</width>
				<height>30</height>
				<align>center</align>
				<font>LightCDNormal</font>
				<label>$INFO[ListItem.Label]</label>
			</control>
			<control type="group">
				<top>55</top>
				<control type="image">
					<include>CircleMenue-Dimension</include>
					<texture colordiffuse="$VAR[PrimaryCol]">rings/inner-ring.png</texture>
					<visible>Control.HasFocus(9000)</visible>
				</control>
				<control type="image">
					<include>CircleMenue-Dimension</include>
					<texture colordiffuse="$VAR[CirclesInnerCol]">rings/inner-ring.png</texture>
					<visible>!Control.HasFocus(9000)</visible>
