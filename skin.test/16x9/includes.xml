	<expression name="VisVideoOverlays">!ListItem.IsParentFolder + [String.Contains(ListItem.DBTYPE,season) | String.Contains(ListItem.DBTYPE,tvshow) | String.Contains(ListItem.DBTYPE,episode) | String.Contains(ListItem.DBTYPE,movie) | String.Contains(ListItem.DBTYPE,video) | String.Contains(ListItem.DBTYPE,set) | String.IsEqual(ListItem.Overlay,OverlayUnwatched.png) | String.IsEqual(ListItem.Overlay,OverlayWatched.png)]</expression>
	<expression name="VisGenre">System.HasAddon(resource.images.moviegenrefanart.cycle) + Container.Content(genres)</expression>
	<expression name="VisSMSScroller">!ListItem.IsParentFolder + [ControlGroup(62).HasFocus | Container.Scrolling]</expression>
	<expression name="VisNoDefaultIcon">!String.StartsWith(ListItem.Icon,Default)</expression>
</includes>