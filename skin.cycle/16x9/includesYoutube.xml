</include>
<include name="YoutubeViewLabel2">
	<top>20</top>
	<width>370</width>
	<height>78</height>
	<wrapmultiline>true</wrapmultiline>
	<autoscroll time="3000" delay="8000" repeat="5000">true</autoscroll>
	<scroll>false</scroll>
	<font>RegularSmall</font>
	<textcolor>TextColorMedium</textcolor>
</include>
<include name="AnimationYoutubeViewImageFO">
	<animation type="Focus">
		<effect type="zoom" time="250" end="105" start="100" center="175,210" />
	</animation>
	<animation type="UnFocus">
		<effect type="zoom" start="105" end="100" time="120" center="175,210" />
	</animation>
</include>
<include name="YoutubeView_vintageoverlay">
