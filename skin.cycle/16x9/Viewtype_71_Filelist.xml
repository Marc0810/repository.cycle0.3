<include name="Viewtype_71_Filelist">
	<control type="group">
		<visible>Control.IsVisible(71)</visible>
		<description>List View (id=71) Dateiliste Addons/Programme/Musik</description>
		<include>Conditional_Left_Slide_Animation395</include>
		<include>Open_Close_Visible_Hidden_Zoom_Animation</include>
		<control type="list" id="71">
			<left>150</left>
			<top>120</top>
			<width>1600</width>
			<height>870</height>
			<onleft>back</onleft>
			<onright condition="Integer.IsGreater(Container(71).NumPages,1)">60</onright>
			<onright condition="!Integer.IsGreater(Container(71).NumPages,1)">noop</onright>
			<onup>71</onup>
			<ondown>71</ondown>
			<pagecontrol>60</pagecontrol>
			<viewtype label="FileList">list</viewtype>
			<scrolltime>400</scrolltime>
			<itemlayout condition="!Container.Content(songs)" width="1520" height="171"><!-- alle auÃŸer songs sortierung titel + rating sonst label2 -->
				<include>Viewtype_71_layout_files</include>
				<include>Viewtype_71_layout_filesArtNF</include>
			</itemlayout>
			<focusedlayout condition="!Container.Content(songs)" width="1520" height="171">
				<control type="image"><!-- Hintergrund -->
					<description>Hintergrund focus streifen</description>
					<top>30</top>
					<width>30</width>
					<height>120</height>
					<texture colordiffuse="$VAR[PrimaryCol]">white.png</texture>
					<animation effect="fade" delay="400" time="200" start="50" end="100">Focus</animation>
					<animation effect="fade" time="200" start="100" end="50">UnFocus</animation>
				</control>
				<control type="image">
					<description>Hintergrund</description>
					<left>30</left>
					<top>30</top>
					<width>1490</width>
					<height>120</height>
					<texture colordiffuse="ListBackgroundDiffuse">white.png</texture>
					<animation effect="fade" delay="400" time="200" start="50" end="100">Focus</animation>
					<animation effect="fade" time="200" start="100" end="50">UnFocus</animation>>
				</control>
				<control type="image">
					<description>Hintergrund diffuse nach scrollen</description>
					<left>30</left>
					<top>30</top>
					<width>1490</width>
					<height>120</height>
					<texture colordiffuse="ListBackgroundDiffuse">white.png</texture>
					<animation effect="fade" delay="800" time="200" start="0" end="100">Focus</animation>
					<animation effect="fade" time="200" start="100" end="0">UnFocus</animation>
				</control>
				<include>Viewtype_71_layout_files</include>
				<include>Viewtype_71_layout_filesArtFO</include>
			</focusedlayout>
			<itemlayout condition="Container.Content(songs)" width="1520" height="105"><!-- songs sortierung titel + rating sonst label2 -->
				<include>Viewtype_71_layout_songs</include>
			</itemlayout>
			<focusedlayout condition="Container.Content(songs)" width="1520" height="105">
				<control type="image"><!-- Hintergrund -->
					<description>Hintergrund focus streifen</description>
					<left>20</left>
					<top>30</top>
					<width>30</width>
					<height>88.5</height>
					<texture colordiffuse="$VAR[PrimaryCol]">white.png</texture>
					<animation effect="fade" delay="400" time="200" start="50" end="100">Focus</animation>
					<animation effect="fade" time="200" start="100" end="50">UnFocus</animation>
				</control>
				<control type="image">
					<description>Hintergrund</description>
					<left>20</left>
					<top>30</top>
					<width>1500</width>
					<height>88.5</height>
					<texture colordiffuse="ListBackgroundDiffuse">white.png</texture>
					<animation effect="fade" delay="400" time="200" start="50" end="100">Focus</animation>
					<animation effect="fade" time="200" start="100" end="50">UnFocus</animation>
				</control>
				<control type="image">
					<description>Hintergrund diffuse nach scrollen</description>
					<left>20</left>
					<top>30</top>
					<width>1500</width>
					<height>88.5</height>
					<texture colordiffuse="ListBackgroundDiffuse">white.png</texture>
					<animation effect="fade" delay="800" time="200" start="0" end="100">Focus</animation>
					<animation effect="fade" time="200" start="100" end="0">UnFocus</animation>
				</control>
				<include>Viewtype_71_layout_songs</include>
			</focusedlayout>
		</control>
		<control type="scrollbar" id="60"><!-- Scroller -->
			<left>1800</left>
			<top>140</top>
			<width>6</width>
			<height>825</height>
			<onleft>71</onleft>
			<onright condition="[Container.Content(albums) | Container.Content(artists) | Container.Content(songs) | Container.Content(addons)] + [String.IsEqual(Container.SortMethod,$LOCALIZE[556]) | String.IsEqual(Container.SortMethod,$LOCALIZE[558]) | String.IsEqual(Container.SortMethod,$LOCALIZE[557]) | String.IsEqual(Container.SortMethod,$LOCALIZE[578])]">SetFocus(62)</onright><!-- SMS bei sortierung 556=Title 558=Album 557=artist 558=Artist / Year -->
			<onright condition="[Container.Content(albums) | Container.Content(artists) | Container.Content(songs) | Container.Content(addons)] + ![String.IsEqual(Container.SortMethod,$LOCALIZE[556]) | String.IsEqual(Container.SortMethod,$LOCALIZE[558]) | String.IsEqual(Container.SortMethod,$LOCALIZE[557]) | String.IsEqual(Container.SortMethod,$LOCALIZE[578])]">71</onright><!-- back bei nicht album/song/addon/artist  -->
			<onright condition="![Container.Content(albums) | Container.Content(artists) | Container.Content(songs) | Container.Content(addons)]">71</onright><!-- back bei nicht album/song/addon/artist -->
		</control>
		<control type="group"><!-- SMS Scroller -->
			<description>SMS Bar</description>
			<top>20</top>
			<include>SMSScrollbar</include>
		</control>
		<control type="group"><!-- Sortletter und SMS Jumpletter -->
			<description>SMS label + icon</description>
			<left>1767</left>
			<top>110</top>
			<visible>$EXP[SMSScrollerFO]</visible>
			<include>SMSLetter_inout_Animation</include>
			<include>ScrollSMSLetter</include>
		</control>
	</control>
</include>
