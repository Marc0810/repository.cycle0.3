<include name="Viewtype_71_layout_filesArtNF">
	<control type="group"><!-- Artworks -->
		<left>20</left>
		<top>20</top>
		<control type="image">
			<description>Border</description>
			<left>-4</left>
			<top>-4</top>
			<width>148</width>
			<height>148</height>
			<texture border="8" colordiffuse="$VAR[CirclesCol]">views/white8.png</texture>
		</control>
		<control type="image"><!-- Fallback -->
			<description>Hintergrund Farbe</description>
			<width>140</width>
			<height>140</height>
			<texture background="true" colordiffuse="$VAR[PrimaryCol]">white.png</texture>
		</control>
		<control type="label">
			<description>Fallback icon</description>
			<width>140</width>
			<height>140</height>
			<align>center</align>
			<font>Iconfilelistdefault</font>
			<textcolor>$VAR[CirclesCol]</textcolor>
			<label>$VAR[DefaultIcons]</label>
		</control>
		<control type="image">
			<description>Art</description>
			<width>140</width>
			<height>140</height>
			<texture background="true">$INFO[ListItem.Icon]</texture>
			<aspectratio>scale</aspectratio>
			<visible>!ListItem.IsParentFolder + !String.StartsWith(ListItem.Icon,Default)</visible>
		</control>
		<control type="image"><!-- Inaktiv overlay -->
			<width>140</width>
			<height>140</height>
			<texture colordiffuse="$VAR[InaktivImageDiffuse]">white.png</texture>
		</control>
		<include content="texture_vintageoverlay" condition="Skin.HasSetting(EnableVintageOverlay)"><!-- Vintage overlay -->
			<param name="width">140</param>
			<param name="height">140</param>
			<param name="texture">views/oldpaperoverlay2.png</param>
		</include>
	</control>
</include>
