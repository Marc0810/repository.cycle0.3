<onload condition="String.IsEmpty(skin.string(MainslotWidgetTitle))">skin.setstring(MainslotWidgetTitle,Begonnene Serien)</onload>
<onload condition="String.IsEmpty(skin.string(MainslotWidgetTitle))">skin.setstring(MainslotWidgetContent,plugin://service.library.data.provider?type=recommendedepisodes&amp;reload=$INFO[Window(Home).Property(recommendedepisodes)])</onload>
<onload condition="String.IsEmpty(skin.string(VideoslotWidgetTitle))">skin.setstring(VideoslotWidgetTitle,Begonnene Serien)</onload>
<onload condition="String.IsEmpty(skin.string(VideoslotWidgetTitle))">skin.setstring(VideoslotWidgetContent,plugin://service.library.data.provider?type=recommendedepisodes&amp;reload=$INFO[Window(Home).Property(recommendedepisodes)])</onload>
<!-- View Defaults setzen -->
<onload condition="String.IsEmpty(skin.string(FilmViewtype))">skin.setstring(FilmViewtype,Poster)</onload>
<onload condition="String.IsEmpty(skin.string(TVViewtype))">skin.setstring(TVViewtype,Landscape)</onload>
<onload condition="String.IsEmpty(skin.string(SetViewtype))">skin.setstring(SetViewtype,Poster)</onload>
<onload condition="String.IsEmpty(skin.string(GenreViewtype))">skin.setstring(GenreViewtype,Poster)</onload>
<onload condition="String.IsEmpty(skin.string(YearsViewtype))">skin.setstring(YearsViewtype,Liste)</onload>
<onload condition="String.IsEmpty(skin.string(TagsViewtype))">skin.setstring(TagsViewtype,Liste)</onload>
<onload condition="String.IsEmpty(skin.string(ActorViewtype))">skin.setstring(ActorViewtype,Poster)</onload>
<onload condition="String.IsEmpty(skin.string(PLS_MovieViewtype))">skin.setstring(PLS_MovieViewtype,Banner)</onload>
<onload condition="String.IsEmpty(skin.string(PLS_TVViewtype))">skin.setstring(PLS_TVViewtype,Poster)</onload>
<onload condition="String.IsEmpty(skin.string(PLS_EPViewtype))">skin.setstring(PLS_EPViewtype,Liste)</onload>
<controls>
