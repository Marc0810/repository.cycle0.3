		<onfocus condition="String.StartsWith(Container.Folderpath,videodb://tvshows/genres/)">SetProperty(PreviouseWindowVideos,ActivateWindow(videos,tvshowgenres,return),videos)</onfocus><!-- serie switch back prop -->
