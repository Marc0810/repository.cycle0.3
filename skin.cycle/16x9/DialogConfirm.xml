<?xml version="1.0" encoding="UTF-8"?>
<window>
<onunload condition="!Skin.HasSetting(FirstTimeRun)">ActivateWindow(1104)</onunload><!-- first start -->
<!-- Cycle 0.3 -->
<coordinates>
	<system>1</system>
	<left>0</left>
	<top>0</top>
</coordinates>
<zorder>236</zorder><!-- zorder erhoehung für slideshow layout -->
<controls>
<control type="progress" id="20"><!-- progress visible geht nicht für zweierlei layouts -->
	<include>HiddenControl</include>
	<info>System.Progressbar</info>
</control>
<control type="grouplist" id="9000">
	<orientation>horizontal</orientation>
	<top>-200</top>
	<width>915</width>
	<align>center</align>
	<control type="button" id="11">
		<description>Ja button</description>
		<onclick condition="!Skin.HasSetting(FirstTimeRun)">ActivateWindow(1104)</onclick>
		<include>HiddenControl</include>
		<label></label>
	</control>
	<control type="button" id="10">
		<description>Nein button</description>
		<include>HiddenControl</include>
		<label></label>
	</control>
	<control type="button" id="12">
		<description>Reserve button</description>
		<include>HiddenControl</include>
		<label></label>
	</control>
</control>
<control type="label" id="1">
	<description>Titel</description>
	<include>HiddenControl</include>
	<label></label>
</control>
<control type="textbox" id="9">
	<description>Text</description>
	<label></label>
	<include>HiddenControl</include>
</control>
<control type="group"><!-- ok|yes/no default Layout -->
	<left>625</left>
	<top>380</top>
	<include>Dialog_popup_Animation</include>
	<visible>Window.Is(yesnodialog) | Window.Is(okdialog)</visible>
	<control type="image"><!-- Hintergrund -->
		<description>Hintergrund Dialog</description>
		<left>dialogbar.left</left>
		<top>dialogbar.top</top>
		<width>664</width>
		<height>320</height>
		<include>DialogBackground</include>
	</control>
	<control type="image">
		<description>Hintergrund Titel</description>
		<top>dialogbar.top</top>
		<width>670</width>
		<height>75</height>
		<texture border="14,0,14,0" colordiffuse="$VAR[primaryCol]" flipy="true" background="true">cycle/full-11.png</texture>
	</control>
	<control type="label"><!-- content -->
		<description>Titel</description>
		<left>bottom.dialogbar.left</left>
		<top>dialogbar.top</top>
		<width>634</width>
		<height>75</height>
		<font>DialogHeader</font>
		<textcolor>TextColorFO</textcolor>
		<label>$INFO[Control.GetLabel(1)]</label>
		<include>VintageDialogTitel_Animation</include>
	</control>
	<control type="textbox">
		<description>Text</description>
		<left>bottom.dialogbar.left</left>
		<top>70</top>
		<width>634</width>
		<height max="130">auto</height>
		<font>RegularSmall</font>
		<label>$INFO[Control.GetLabel(9)]</label>
		<autoscroll time="3000" delay="4000" repeat="5000">true</autoscroll>
	</control>
	<control type="grouplist">
		<orientation>horizontal</orientation>
		<left>105</left>
		<top>230</top>
		<width>454</width>
		<align>center</align>
		<itemgap>50</itemgap>
		<control type="image">
			<description>Ja button focus</description>
			<width>200</width>
			<height>50</height>
			<texture colordiffuse="$VAR[primaryCol]">dialogs/DialokOKButton.png</texture>
			<label>$INFO[Control.GetLabel(11)]</label>
			<visible>Control.IsVisible(11) + Control.HasFocus(11)</visible>
		</control>
		<control type="image">
			<description>Ja button unfocus</description>
			<width>200</width>
			<height>50</height>
			<texture colordiffuse="$VAR[secondaryCol]">dialogs/DialokOKButtonNF.png</texture>
			<label>$INFO[Control.GetLabel(11)]</label>
			<visible>Control.IsVisible(11) + !Control.HasFocus(11)</visible>
		</control>
		<control type="image">
			<description>Nein button focus</description>
			<width>200</width>
			<height>50</height>
			<texture colordiffuse="$VAR[primaryCol]">dialogs/DialokOKButton.png</texture>
			<label>$INFO[Control.GetLabel(10)]</label>
			<visible>Control.IsVisible(10) + Control.HasFocus(10)</visible>
		</control>
		<control type="image">
			<description>Nein button unfocus</description>
			<width>200</width>
			<height>50</height>
			<texture colordiffuse="$VAR[secondaryCol]">dialogs/DialokOKButtonNF.png</texture>
			<label>$INFO[Control.GetLabel(10)]</label>
			<visible>Control.IsVisible(10) + !Control.HasFocus(10)</visible>
		</control>
	</control>
	<control type="grouplist">
		<orientation>horizontal</orientation>
		<left>105</left>
		<top>230</top>
		<width>454</width>
		<align>center</align>
		<itemgap>50</itemgap>
		<control type="label">
			<description>Ja button label focus</description>
			<width>200</width>
			<height>50</height>
			<align>center</align>
			<font>RegularSmall</font>
			<textcolor>TextColorFO</textcolor>
			<label>$INFO[Control.GetLabel(11)]</label>
			<visible>Control.IsVisible(11)</visible>
		</control>
		<control type="label">
			<description>Nein button label focus</description>
			<width>200</width>
			<height>50</height>
			<align>center</align>
			<font>RegularSmall</font>
			<textcolor>TextColorFO</textcolor>
			<label>$INFO[Control.GetLabel(10)]</label>
			<visible>Control.IsVisible(10)</visible>
		</control>
	</control>
</control>
<control type="group"><!-- progress fake Layout -->
	<left>1080</left>
	<top>-137</top>
	<visible>Window.Is(progressdialog) + !Window.IsVisible(Custom_1150_Screensaver.xml) + !Window.IsVisible(startup) + !Window.IsVisible(DialogNotification.xml) + !Window.IsActive(extendedprogressdialog) + !Window.IsActive(busydialog) + !Window.IsVisible(Custom_1118_Histroyloader.xml)</visible>
	<include>Dialog_topdown_Animation</include>
	<animation type="Conditional" condition="Window.IsVisible(volumebar) + !Player.Muted" reversible="true">
		<effect type="fade" start="100" end="0" time="120" tween="cubic" easing="out" />
	</animation>
	<control type="label">
		<description>Titel</description>
		<top>22</top>
		<width>665</width>
		<height>27</height>
		<align>right</align>
		<font>BoldSmall</font>
		<textcolor>$VAR[OSDDialogColor]</textcolor>
		<label>$INFO[Control.GetLabel(1)]</label>
		<visible>!Window.IsVisible(Custom_1150_Screensaver.xml)</visible>
	</control>
	<control type="textbox">
		<description>Text</description>
		<top>52</top>
		<width>665</width>
		<height>48</height>
		<align>right</align>
		<font>RegularSmall</font>
		<textcolor>$VAR[OSDDialogColor]</textcolor>
		<label>$INFO[Control.GetLabel(9)]</label>
		<visible>!Window.IsVisible(Custom_1150_Screensaver.xml)</visible>
	</control>
	<include content="DialogDefaultProgresskringel">
		<param name="id">20</param>
	</include>
</control>
<control type="group"><!-- progress Slideshow Layout -->
	<left>800</left>
	<top>970</top>
	<visible>Window.Is(progressdialog) + Window.IsVisible(Custom_1150_Screensaver.xml)</visible>
	<animation type="Visible" reversible="false">
		<effect type="fade" delay="1000" time="600" start="0" end="100" tween="cubic" easing="out" />
	</animation>
	<animation type="Hidden" reversible="false">
		<effect type="fade" time="100" start="100" end="0" tween="cubic" easing="out" />
	</animation>
	<control type="group">
		<left>-660</left>
		<include content="DialogDefaultProgresskringel">
			<param name="id">20</param>
		</include>
	</control>
	<control type="label">
		<description>Titel</description>
		<left>100</left>
		<top>22</top>
		<width>665</width>
		<height>27</height>
		<font>BoldSmall</font>
		<textcolor>$VAR[OSDDialogColor]</textcolor>
		<label>$INFO[Control.GetLabel(1)]</label>
	</control>
	<control type="textbox">
		<description>Text</description>
		<left>100</left>
		<top>52</top>
		<width>665</width>
		<height>30</height>
		<font>RegularSmall</font>
		<textcolor>$VAR[OSDDialogColor]</textcolor>
		<label>$INFO[Control.GetLabel(9)]</label>
	</control>
</control>
</controls>
</window>