<?xml version="1.0" encoding="UTF-8"?>
<window>
<!-- Cycle 0.3 -->
<coordinates>
	<system>1</system>
	<origin x="0" y="0">!Window.IsVisible(home)</origin>
	<origin x="0" y="10">Window.IsVisible(home)</origin>
</coordinates>
<zorder>439</zorder><!-- zorder erhoehung fÃ¼r slideshow layout -->
<controls>
<control type="label" id="30">
	<description>Busy Label</description>
	<include>HiddenControl</include>
	<label>...Arbeitet</label>
</control>
<control type="textbox" id="31">
	<include>HiddenControl</include>
	<label>bitte warten</label>
</control>
<control type="progress" id="10">
	<include>HiddenControl</include>
</control>
<control type="group">
	<left>1090</left>
	<top>-137</top>
	<include>Dialog_topdown_Animation</include>
	<animation type="Conditional" condition="[Window.IsVisible(volumebar) + !Player.Muted] | Window.IsVisible(Custom_1150_Screensaver.xml) | Window.IsVisible(startup) | Window.IsVisible(Notification) | Window.IsActive(extendedprogressdialog) | Window.IsActive(DialogConfirm.xml) | Window.IsVisible(seekbar) | Window.IsVisible(Custom_1118_Histroyloader.xml)" reversible="true">
		<effect type="fade" start="100" end="0" time="120" tween="cubic" easing="out" />
	</animation>
	<include content="DialogDefaultProgresskringel">
		<param name="id">10</param>
		<param name="label1id">30</param>
		<param name="label2id">31</param>
	</include>
</control>
<control type="group"><!-- Slideshow Layout -->
	<left>800</left>
	<top>960</top>
	<visible>Window.IsVisible(Custom_1150_Screensaver.xml)</visible>
	<animation type="Visible" reversible="false">
		<effect type="fade" delay="1000" time="600" start="0" end="100" tween="cubic" easing="out" />
	</animation>
	<animation type="Hidden" reversible="false">
		<effect type="fade" time="100" start="100" end="0" tween="cubic" easing="out" />
	</animation>
	<control type="group">
		<left>-300</left>
		<include content="DialogDefaultProgresskringel">
			<param name="id">10</param>
			<param name="label1id">30</param>
			<param name="label2id">31</param>
		</include>
	</control>
</control>
</controls>
</window>