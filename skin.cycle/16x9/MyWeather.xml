<?xml version="1.0" encoding="UTF-8"?>
<window>
<!-- Cycle 0.3 -->
<defaultcontrol always="true">54</defaultcontrol>
<menucontrol>9900</menucontrol>
<controls>
	<control type="button" id="54">
		<include>HiddenControl</include>
		<onup>51</onup>
		<ondown>51</ondown>
		<onleft>Action(PreviousMenu)</onleft>
		<onright>Action(PreviousMenu)</onright>
	</control>
	<control type="spincontrolex" id="3">
		<description>Change location button</description>
		<include>HiddenControl</include>
		<label>-</label>
	</control>
	<include condition="Skin.HasSetting(EnableVintageOverlay)">global_vintagebackground</include>
	<include condition="!Skin.HasSetting(EnableVintageOverlay)">global_background</include>
	<control type="group"><!-- breadcumb -->
		<include>global_topbar</include>
		<animation type="Conditional" condition="Window.IsVisible(Custom_1120_MainMenuSlide.xml)">
			<effect type="slide" end="395,0" time="180" delay="180" tween="quadratic" easing="out" reversible="true" />
		</animation>
	</control>
	<control type="group"><!-- views -->
		<description>Views</description>
		<control type="group" id="50">
			<include>Conditional_Viewinaktiv_Zoom_Animation</include>
			<include>WeatherDaily</include>
			<include>WeatherCurrent</include>
			<control type="group"><!-- info kein wetter -->
				<left>760</left>
				<top>520</top>
				<visible>!Weather.IsFetched | IsEmpty(Window(Weather).Property(WeatherProvider))</visible>
				<control type="label">
					<width>400</width>
					<height>200</height>
					<align>center</align>
					<font>weatherloading</font>
					<textcolor>red</textcolor>
					<label>Ë</label>
					<visible>!Weather.IsFetched</visible>
				</control>
				<control type="label">
					<top>380</top>
					<width>400</width>
					<height>80</height>
					<align>center</align>
					<label>suche Wetterbericht</label>
					<wrapmultiline>true</wrapmultiline>
					<visible>!Weather.IsFetched + !IsEmpty(Window(Weather).Property(WeatherProvider))</visible>
				</control>
				<control type="label">
					<top>380</top>
					<width>400</width>
					<height>80</height>
					<align>center</align>
					<label>kein Addon ist ausgewählt</label>
					<wrapmultiline>true</wrapmultiline>
					<visible>!Weather.IsFetched + IsEmpty(Window(Weather).Property(WeatherProvider))</visible>
				</control>
			</control>
		</control>
	</control>
	<control type="group"><!-- Logo Quicknav 9900 -->
		<description>Logo Quicknav 9900</description>
		<left>760</left>
		<top>340</top>
		<width>400</width>
		<height>400</height>
		<visible allowhiddenfocus="true">Control.HasFocus(9900)</visible>
		<include>Open_Close_Visible_Hidden_Zoom_Animation</include>
		<include>LogoMenueLayout</include>
		<control type="label">
			<description>Linker Button indikator</description>
			<left>-150</left>
			<top>170</top>
			<width>75</width>
			<height>75</height>
			<align>center</align>
			<font>BoldBig</font>
			<textcolor>$VAR[secondaryCol]</textcolor>
			<label> ‹‹</label>
			<visible>Control.HasFocus(9900) + Control.IsVisible(9902)</visible>
		</control>
		<control type="label">
			<description>Rechter Button indikator</description>
			<left>450</left>
			<top>170</top>
			<width>75</width>
			<height>75</height>
			<align>center</align>
			<font>BoldBig</font>
			<textcolor>$VAR[secondaryCol]</textcolor>
			<label> ››</label>
			<visible>Control.HasFocus(9900) + Control.IsVisible(9901)</visible>
		</control>
		<control type="button" id="9900">
			<description>Startbutton ansicht refresh</description>
			<width>400</width>
			<height>400</height>
			<align>center</align>
			<font>IconHome</font>
			<textcolor>$VAR[circlesCol]</textcolor>
			<focusedcolor>$VAR[circlesCol]</focusedcolor>
			<onleft condition="Control.IsVisible(9902)">SetFocus(9902)</onleft>
			<onright condition="Control.IsVisible(9901)">SetFocus(9901)</onright>
			<onup>54</onup>
			<ondown>54</ondown>
			<include>CycleMyWeatherQuicknavClick</include>
			<label>$VAR[CycleMyWeatherQuicknavTitle]</label>
		</control>
		<control type="button" id="9902">
			<description>Linker Button menue</description>
			<left>-600</left>
			<width>400</width>
			<height>400</height>
			<align>center</align>
			<font>IconHomeNF</font>
			<textcolor>$VAR[primaryCol]</textcolor>
			<focusedcolor>$VAR[primaryCol]</focusedcolor>
			<label>$VAR[CycleMyWeatherQuicknavTitleL]</label>
			<include>CycleMyWeatherQuicknavClickL</include>
			<animation type="Conditional" condition="Control.HasFocus(9900)">
				<effect type="fade" start="10" end="10" time="0" />
			</animation>
		</control>
		<control type="button" id="9901">
			<description>Rechter Button Setting</description>
			<left>600</left>
			<width>400</width>
			<height>400</height>
			<align>center</align>
			<font>IconHome</font>
			<textcolor>$VAR[primaryCol]</textcolor>
			<focusedcolor>$VAR[primaryCol]</focusedcolor>
			<label>$VAR[CycleMyWeatherQuicknavTitleR]</label>
			<include>CycleMyWeatherQuicknavClickR</include>
			<animation type="Conditional" condition="Control.HasFocus(9900)">
				<effect type="fade" start="10" end="10" time="0" />
			</animation>
		</control>
	</control>
	<control type="group"><!-- logo text -->
		<include>Open_Close_Visible_Hidden_Fade_Animation</include>
		<visible>Control.HasFocus(9900)</visible>
		<control type="label">
			<description>Titel skin label</description>
			<left>460</left>
			<top>720</top>
			<width>1000</width>
			<height>60</height>
			<font>CycleLogoBranding</font>
			<textcolor>TextColorMedium</textcolor>
			<align>center</align>
			<label>$VAR[GlobalSkinversion]</label>
		</control>
		<control type="label">
			<description>Subtitel provider</description>
			<left>460</left>
			<top>870</top>
			<width>1000</width>
			<height>60</height>
			<align>center</align>
			<font>BreadcumbNormal</font>
			<textcolor>TextColorMedium</textcolor>
			<label>$INFO[Window(Weather).Property(WeatherProvider)]</label>
		</control>
		<control type="label">
			<description>Subinfo aktualisierungszeit</description>
			<left>460</left>
			<top>920</top>
			<width>1000</width>
			<height>60</height>
			<align>center</align>
			<font>RegularSmall</font>
			<textcolor>TextColorMedium</textcolor>
			<label>$INFO[Window(Weather).Property(Updated),Update: ,]</label>
		</control>
		<control type="label">
			<description>Subinfo kurzausblick und temperatur</description>
			<left>460</left>
			<top>990</top>
			<width>1000</width>
			<height>60</height><!-- check -->
			<align>center</align>
			<font>RegularSmall</font>
			<textcolor>TextColorMedium</textcolor>
			<label>$INFO[Window(Weather).Property(Current.Condition)]$VAR[CustomEnumeration]$INFO[Window(Weather).Property(Current.Temperature)]$INFO[System.TemperatureUnits]</label>
		</control>
	</control>
</controls>
</window>
