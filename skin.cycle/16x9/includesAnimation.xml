	<animation effect="fade" time="600">Hidden</animation>
</include>

<!-- Open/Close slide Animation -->
<include name="Open_Close_Visible_Hidden_Slide_Animation375">
	<animation type="WindowOpen">
		<effect type="fade" start="0" end="100" time="280" delay="180" />
		<effect type="slide" start="-375,0" time="300" delay="180" tween="quadratic" easing="out"/>
	</animation>
	<animation type="WindowClose">
		<effect type="fade" start="100" end="0" time="280" />
		<effect type="slide" end="-375,0" time="300" tween="quadratic" easing="out"/>
	</animation>
	<animation type="Visible">
		<effect type="fade" start="0" end="100" time="280" delay="180" />
		<effect type="slide" start="-375,0" time="300" delay="180" tween="quadratic" easing="out"/>
	</animation>
	<animation type="Hidden">
		<effect type="fade" start="100" end="0" time="280" />
		<effect type="slide" end="-375,0" time="300" tween="quadratic" easing="out"/>
	</animation>
</include>

