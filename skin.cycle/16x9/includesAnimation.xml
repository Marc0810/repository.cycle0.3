<?xml version="1.0" encoding="UTF-8"?>
<includes>
<!-- Cycle 0.3 -->
<include name="VintageDialogTitel_Animation">
	<animation type="Conditional" condition="Skin.HasSetting(VintageFont)" reversible="false">
		<effect type="slide" start="0,-8" end="0,-8" time="0" />
	</animation>
</include>
<include name="VintageFont_Animation-12">
	<animation type="Conditional" condition="Skin.HasSetting(VintageFont)" reversible="false">
		<effect type="slide" start="0,-12" end="0,-12" time="0" />
	</animation>
</include>

<include name="Dialog_topdown_Animation">
	<animation type="WindowOpen" reversible="false">
		<effect type="slide" start="0,0" end="0,150" time="400" tween="cubic" easing="out" />
		<effect type="fade" start="0" end="100" time="400" />
	</animation>
	<animation type="WindowClose" reversible="false">
		<effect type="fade" start="100" end="0" time="100" />
	</animation>
	<animation type="Conditional" condition="Window.IsVisible(fullscreeninfo) | Window.IsVisible(seekbar) | Window.IsVisible(videoosd) | Window.IsVisible(Custom_1151_VideoOverlay.xml) | [Window.IsVisible(visualisation) + Player.ShowInfo]" reversible="true">
		<effect type="slide" start="0,0" end="0,10" time="400" tween="cubic" easing="out" />
	</animation>
</include>

<include name="Dialog_popup_Animation">
	<animation type="WindowOpen">
		<effect type="slide" start="-40,0" end="0,0" time="500" easing="inout" tween="cubic"/>
		<effect type="fade" start="0" end="100" time="400"/>
	</animation>
	<animation type="Visible">
		<effect type="slide" start="-40,0" end="0,0" time="500" easing="inout" tween="cubic"/>
		<effect type="fade" start="0" end="100" time="400"/>
	</animation>
	<animation type="WindowClose">
		<effect type="slide" start="0,0" end="40,0" time="300" easing="inout" tween="cubic"/>
		<effect type="fade" start="100" end="0" time="300"/>
	</animation>
	<animation type="Hidden">
		<effect type="slide" start="0,0" end="40,0" time="300" easing="inout" tween="cubic"/>
		<effect type="fade" start="100" end="0" time="250"/>
	</animation>
</include>

<include name="Conditional_Viewinaktiv_Zoom_Animation">
	<animation type="conditional" reversible="true" condition="Control.HasFocus(9900)">
		<effect type="fade" start="100" end="30" time="150" tween="cubic" easing="inout" />
		<effect type="zoom" start="100" end="70" time="150" tween="cubic" easing="inout" center="960,540"/>
	</animation>
</include>

<include name="OSDDialog_popup_Animation">
	<animation type="WindowOpen" reversible="false">
		<effect type="slide" start="0,85" end="0" time="200" tween="sine" easing="inout" />
		<effect type="fade" start="0" end="100" time="200" tween="sine" easing="inout" />
	</animation>
	<animation type="Visible" reversible="false">
		<effect type="slide" start="0,85" end="0" time="200" tween="sine" easing="inout" />
		<effect type="fade" start="0" end="100" time="200" tween="sine" easing="inout" />
	</animation>
	<animation type="WindowClose" reversible="false">
		<effect type="slide" end="0,115" start="0" time="200" tween="sine" easing="inout" />
		<effect type="fade" end="0" start="100" time="200" tween="sine" easing="inout" />
	</animation>
	<animation type="Hidden" reversible="false">
		<effect type="slide" end="0,115" start="0" time="200" tween="sine" easing="inout" />
		<effect type="fade" end="0" start="100" time="200" tween="sine" easing="inout" />
	</animation>
</include>

<include name="Dimmer_Animation">
	<animation type="Focus">
