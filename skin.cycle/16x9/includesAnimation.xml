<include name="Conditional_Inaktiv_Fade_Animation">
	<animation type="Conditional" condition="![Window.IsActive(contextmenu) |Window.IsActive(virtualkeyboard) | Window.IsActive(networksetup) | Window.IsActive(numericinput) | Window.IsActive(mediasource) | Window.IsActive(smartplaylisteditor) | Window.IsActive(smartplaylistrule) | Window.IsActive(addonsettings) | Window.IsActive(DialogSettings.xml) | Window.IsActive(DialogConfirm.xml)]">
