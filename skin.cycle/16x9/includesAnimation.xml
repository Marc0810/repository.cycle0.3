<include name="Conditional_Left_Slide_Animation395">
	<animation type="Conditional" condition="!Window.IsVisible(playercontrols) + [ControlGroup(9110).HasFocus | ControlGroup(9300).HasFocus | Window.IsActive(contextmenu) | Window.IsActive(shutdownmenu) | Window.IsVisible(Custom_1141_ViewtypeSimilar.xml)] + !Window.IsVisible(musicplaylisteditor)">
		<effect type="slide" end="395,0" time="180" delay="180" tween="quadratic" easing="out" reversible="true" />
	</animation>
	<animation type="Conditional" condition="[ControlGroup(9110).HasFocus | ControlGroup(9300).HasFocus] + Window.IsActive(DialogSelect.xml) + String.IsEqual(skin.String(SelectDialog),Search)">
		<effect type="slide" end="250,0" time="180" delay="180" tween="quadratic" easing="out" reversible="true" />
	</animation>
	<animation type="Conditional" condition="Window.IsActive(favourites)">
		<effect type="slide" end="100,0" time="180" delay="180" tween="quadratic" easing="out" reversible="true" />
	</animation>
	<animation type="Conditional" condition="Window.IsVisible(playercontrols)">
		<effect type="slide" end="80,0" time="300" />
	</animation>
</include>

