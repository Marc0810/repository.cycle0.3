	<texturenofocus>-</texturenofocus>
</default>

<default type="resize">
	<left>0</left>
	<top>348</top>
	<width>384</width>
	<height>384</height>
	<texturefocus>-</texturefocus>
	<texturenofocus>-</texturenofocus>
</default>

<default type="edit">
	<left>0</left>
	<top>0</top>
	<textoffsetx>30</textoffsetx>
	<align>left</align>
	<aligny>center</aligny>
	<font>RegularNormal</font>
	<textcolor>TextColorLight</textcolor>
	<disabledcolor>TextColorLight</disabledcolor>
	<invalidcolor>$VAR[SecondaryCol]</invalidcolor>
	<selectedcolor>$VAR[SecondaryCol]</selectedcolor>
	<focusedcolor>TextColorFO</focusedcolor>
	<pulseonselect>false</pulseonselect>
	<texturenofocus>-</texturenofocus>
	<texturefocus border="14,0,14,0" colordiffuse="$VAR[PrimaryCol]">cycle/full-11.png</texturefocus>
</default>

<default type="scrollbar">
	<left>0</left>
	<top>0</top>
	<texturesliderbarfocus colordiffuse="$VAR[PrimaryCol]" border="1">white.png</texturesliderbarfocus>
	<texturesliderbackground colordiffuse="TextColorLight" border="2,0,2,0">buttons/scrollbarback.png</texturesliderbackground>
	<texturesliderbar colordiffuse="TextColorLight" border="1">white.png</texturesliderbar>
	<textureslidernib>-</textureslidernib>
	<textureslidernibfocus>-</textureslidernibfocus>
	<showonepage>false</showonepage>
</default>

</includes>