</default>

<default type="sliderex">
	<left>0</left>
	<top>0</top>
	<textoffsetx>30</textoffsetx>
	<align>left</align>
	<aligny>center</aligny>
	<sliderwidth>150</sliderwidth>
	<sliderheight>20</sliderheight>
	<font>RegularNormal</font>
	<textcolor>TextColorHeavy</textcolor>
	<focusedcolor>TextColorFO</focusedcolor>
	<selectedcolor>$VAR[SecondaryCol]</selectedcolor>
	<pulseonselect>false</pulseonselect>
	<texturefocus>-</texturefocus>
	<texturenofocus>-</texturenofocus>
	<texturesliderbar colordiffuse="TextColorMedium">buttons/slider/slider_bg.png</texturesliderbar>
	<textureslidernib colordiffuse="$VAR[PrimaryCol]">buttons/slider/slidernibnf1.png</textureslidernib>
	<textureslidernibfocus colordiffuse="TextColorFO">buttons/slider/slidernibfo1.png</textureslidernibfocus>
