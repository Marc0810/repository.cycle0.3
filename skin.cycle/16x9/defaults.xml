	<texturedowndisabled colordiffuse="TextColorMedium">buttons/spin/spinbuttondownnf.png</texturedowndisabled>
	<textureup colordiffuse="TextColorMedium">buttons/spin/spinbuttonupnf.png</textureup>
	<texturedown colordiffuse="TextColorMedium">buttons/spin/spinbuttondownnf.png</texturedown>
	<textureupfocus>buttons/spin/spinbuttonupfo.png</textureupfocus>
	<texturedownfocus>buttons/spin/spinbuttondownfo.png</texturedownfocus>
</default>

<default type="slider">
	<left>0</left>
	<top>0</top>
	<textoffsetx>30</textoffsetx>
	<controloffsetx>75</controloffsetx>
	<align>left</align>
	<aligny>center</aligny>
	<font>RegularNormal</font>
	<textcolor>TextColorHeavy</textcolor>
	<disabledcolor>TextColorLight</disabledcolor>
	<focusedcolor>TextColorFO</focusedcolor>
	<selectedcolor>$VAR[SecondaryCol]</selectedcolor>
	<pulseonselect>false</pulseonselect>
	<texturefocus>-</texturefocus>
	<texturenofocus>-</texturenofocus>
	<texturesliderbar colordiffuse="TextColorMedium">buttons/slider/slider_bg.png</texturesliderbar>
	<textureslidernib colordiffuse="$VAR[SecondaryCol]">buttons/slider/slidernibnf1.png</textureslidernib>
	<textureslidernibfocus colordiffuse="$VAR[PrimaryCol]">buttons/slider/slidernibfo1.png</textureslidernibfocus>
