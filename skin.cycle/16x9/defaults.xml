<includes>
<!-- Cycle defaults -->
<default type="image">
	<left>0</left>
	<top>0</top>
	<texture>-</texture>
	<colorkey>0</colorkey>
</default>

<default type="group">
	<left>0</left>
	<top>0</top>
</default>

<default type="label">
	<left>0</left>
	<top>0</top>
	<align>left</align>
	<aligny>center</aligny>
	<scroll>false</scroll>
	<font>RegularNormal</font>
	<textcolor>TextColorHeavy</textcolor>
	<disabledcolor>TextColorLight</disabledcolor>
	<focusedcolor>TextColorFO</focusedcolor>
	<selectedcolor>$VAR[SelectedCol]</selectedcolor>
	<label>-</label>
</default>

<default type="fadelabel">
	<left>0</left>
	<top>0</top>
	<align>left</align>
	<aligny>center</aligny>
	<font>RegularNormal</font>
	<textcolor>TextColorHeavy</textcolor>
	<disabledcolor>TextColorLight</disabledcolor>
	<focusedcolor>TextColorFO</focusedcolor>
	<selectedcolor>$VAR[SelectedCol]</selectedcolor>
</default>

<!-- height 60 align button.top.offsety 10 -->
<!-- height 65 align button.top.offsety 15 -->
<!-- height 70 align button.top.offsety 15 -->
<!-- height 75 align button.top.offsety 20 -->

<default type="button">
	<left>0</left>
	<top>0</top>
	<textoffsetx>30</textoffsetx>
	<align>left</align>
	<aligny>center</aligny>
	<font>RegularNormal</font>
	<textcolor>TextColorHeavy</textcolor>
	<disabledcolor>TextColorLight</disabledcolor>
	<focusedcolor>TextColorFO</focusedcolor>
	<selectedcolor>$VAR[SecondaryCol]</selectedcolor>
	<invalidcolor>$VAR[SecondaryCol]</invalidcolor>
	<pulseonselect>false</pulseonselect>
	<texturefocus>-</texturefocus>
	<texturenofocus>-</texturenofocus>
</default>
	
<default type="radiobutton">
	<left>0</left>
	<top>0</top>
	<textoffsetx>30</textoffsetx>
	<aligny>center</aligny>
	<align>left</align>
	<radiowidth>60</radiowidth>
	<radioheight>60</radioheight>
	<font>RegularNormal</font>
	<textcolor>TextColorHeavy</textcolor>
	<disabledcolor>TextColorLight</disabledcolor>
	<focusedcolor>TextColorFO</focusedcolor>
	<selectedcolor>$VAR[SecondaryCol]</selectedcolor>
	<pulseonselect>false</pulseonselect>
	<textureradioonfocus>buttons/radio/radiobutton_fo.png</textureradioonfocus>
	<textureradioonnofocus colordiffuse="TextColorMedium">buttons/radio/radiobutton_fo.png</textureradioonnofocus>
	<textureradioofffocus>buttons/radio/radiobutton_nf.png</textureradioofffocus>
	<textureradiooffnofocus colordiffuse="TextColorMedium">buttons/radio/radiobutton_nf.png</textureradiooffnofocus>
	<textureradioondisabled colordiffuse="TextColorMedium">buttons/radio/radiobutton_fo.png</textureradioondisabled>
	<textureradiooffdisabled colordiffuse="TextColorMedium">buttons/radio/radiobutton_nf.png</textureradiooffdisabled>
	<texturefocus>-</texturefocus>
	<texturenofocus>-</texturenofocus>
</default>

<default type="togglebutton">
	<left>0</left>
	<top>0</top>
	<textoffsetx>30</textoffsetx>
	<align>left</align>
	<aligny>center</aligny>
	<font>RegularNormal</font>
	<textcolor>TextColorHeavy</textcolor>
	<disabledcolor>TextColorLight</disabledcolor>
	<focusedcolor>TextColorFO</focusedcolor>
	<selectedcolor>$VAR[SecondaryCol]</selectedcolor>
	<label>-</label>
	<pulseonselect>false</pulseonselect>
	<alttexturefocus>buttons/radio/radiobutton_fo.png</alttexturefocus>
	<alttexturenofocus colordiffuse="TextColorMedium">buttons/radio/radiobutton_fo.png</alttexturenofocus>
	<texturefocus>buttons/radio/radiobutton_nf.png</texturefocus>
	<texturenofocus colordiffuse="TextColorMedium">buttons/radio/radiobutton_nf.png</texturenofocus>
</default>

<default type="textbox">
	<left>0</left>
	<top>0</top>
	<align>left</align>
	<autoscroll>false</autoscroll>
	<font>RegularNormal</font>
	<textcolor>TextColorHeavy</textcolor>
	<disabledcolor>TextColorHeavy</disabledcolor>
	<focusedcolor>TextColorHeavy</focusedcolor>
	<selectedcolor>$VAR[SecondaryCol]</selectedcolor>
</default>

<default type="progress">
	<left>0</left>
	<top>0</top>
	<info>Player.Progress</info>
	<reveal>true</reveal>
	<texturebg colordiffuse="$VAR[PrimaryCol]">dialogs/progress/bg.png</texturebg>
	<lefttexture colordiffuse="$VAR[PrimaryCol]">dialogs/progress/left.png</lefttexture>
	<midtexture colordiffuse="$VAR[PrimaryCol]">dialogs/progress/left.png</midtexture>
	<righttexture colordiffuse="$VAR[SecondaryCol]">dialogs/progress/right.png</righttexture>
	<overlaytexture>-</overlaytexture>
</default>

<default type="spincontrol">
	<left>0</left>
	<top>0</top>
	<textoffsetx>30</textoffsetx>
	<spinwidth>32</spinwidth>
	<spinheight>70</spinheight>
	<align>left</align>
	<aligny>center</aligny>
	<font>RegularNormal</font>
	<textcolor>TextColorHeavy</textcolor>
	<disabledcolor>TextColorLight</disabledcolor>
	<focusedcolor>TextColorFO</focusedcolor>
	<selectedcolor>$VAR[SecondaryCol]</selectedcolor>
	<pulseonselect>false</pulseonselect>
	<reverse>yes</reverse>
	<texturefocus>-</texturefocus>
	<texturenofocus>-</texturenofocus>
	<textureupdisabled colordiffuse="TextColorMedium">buttons/spin/spinbuttonupnf.png</textureupdisabled>
	<texturedowndisabled colordiffuse="TextColorMedium">buttons/spin/spinbuttondownnf.png</texturedowndisabled>
	<textureup colordiffuse="TextColorMedium">buttons/spin/spinbuttonupnf.png</textureup>
	<texturedown colordiffuse="TextColorMedium">buttons/spin/spinbuttondownnf.png</texturedown>
	<textureupfocus>buttons/spin/spinbuttonupfo.png</textureupfocus>
	<texturedownfocus>buttons/spin/spinbuttondownfo.png</texturedownfocus>
</default>

<default type="spincontrolex">
	<left>0</left>
	<top>0</top>
	<textoffsetx>30</textoffsetx>
	<spinwidth>32</spinwidth>
	<spinheight>70</spinheight>
	<align>left</align>
	<aligny>center</aligny>
	<font>RegularNormal</font>
	<textcolor>TextColorHeavy</textcolor>
	<disabledcolor>TextColorLight</disabledcolor>
	<focusedcolor>TextColorFO</focusedcolor>
	<selectedcolor>$VAR[SecondaryCol]</selectedcolor>
	<pulseonselect>false</pulseonselect>
	<reverse>yes</reverse>
	<texturefocus>-</texturefocus>
	<texturenofocus>-</texturenofocus>
