<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<smartplaylist type="movies">
    <name>Filme - Sortierung Playcount</name>
    <match>all</match>
    <rule field="playcount" operator="greaterthan">
        <value>3</value>
    </rule>
    <order direction="ascending">playcount</order>
</smartplaylist>